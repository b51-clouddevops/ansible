[Unit]
Description = Catalogue Service
[Service]
User=roboshop
Environment=MONGO=true
Environment=MONGO_URL="mongodb://{{DOCDB_USERNAME}}:{{DOCDB_PASSWORD}}@{{MONGODBURL}}:27017/catalogue?tls=true&replicaSet=rs0&readPreference=secondaryPreferred&retryWrites=false";
ExecStart=/bin/node /home/roboshop/catalogue/server.js
SyslogIdentifier=catalogue

[Install]
WantedBy=multi-user.target 

'mongodb://mongodb:27017/catalogue'